        insert offering2;
        affls = [
            SELECT Contact__c, Account__c, Role__c, StartDate__c, EndDate__c
            FROM Affiliation__c
            WHERE Contact__r.Id = :faculty.Id AND Role__c = 'Faculty'
        ];
        System.assertEquals(1, affls.size());
